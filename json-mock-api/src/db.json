    {
      "id": "b367712f-05ec-41b8-9fdc-b55c397afda2",
      "name": "Chicken Katsu Burgers",
      "website": "MOB Kitcen",
      "websiteLogo": "/static/images/placeholders/logo/mob.png",
      "image": "/static/images/cards/katsu-burgers.png",
      "lastMade": "05/10/2021",
      "websiteLink": "https://www.mob.co.uk/recipes/mobs-chicken-katsu-burger",
      "rating": 8.3,
      "cuisine": "Japanese ðŸ‡¯ðŸ‡µ",
      "category": {
        "id": "e92caa30-091b-4ed3-a87b-dff9e09caa17",
        "name": "Treat ðŸŽ©"
      },
      "ingredients": [
        {
          "id": "1a902747-5d4c-49a8-b6c6-d4e653da448c",
          "label": "4 Brioche Baps"
        },
        {
          "id": "03c44661-3e17-471d-9ebe-b10d070d0ddf",
          "label": "Bunch of Coriander"
        },
        { "id": "7652455a-5935-48d2-8bf1-fb3e22d5b77b", "label": "2 Eggs" },
        {
          "id": "07430653-c882-4692-8f0c-2f0427d9a534",
          "label": "Breadcrumbs"
        },
        {
          "id": "b0141331-817d-4923-a40b-36c43530f29e",
          "label": "2 Tbsp Flour"
        },
        {
          "id": "9043df5c-36df-4bd3-addf-c062a114e595",
          "label": "2 Tsp Sesame Oil"
        },
        { "id": "ec565f79-82ff-4205-9d24-f19e979b5baa", "label": "1 Lime" },
        {
          "id": "07c0f548-7eaf-4e6b-8a8e-fab81a38fd1d",
          "label": "1/2 White Cabbage"
        },
        {
          "id": "bd77399b-0ed1-4be1-9807-c08c822de4f2",
          "label": "200g radishes"
        },
        {
          "id": "669a3f93-9cac-4d26-b2c9-7f3ae82c1c82",
          "label": "1 Tbsp Runny Honey"
        },
        {
          "id": "88f9986c-758e-44c9-b237-34d8335845c8",
          "label": "2 Tsp Curry Powder"
        },
        {
          "id": "eb5d158b-2609-449d-beb1-40a982d176a9",
          "label": "1 Tsp Garam Masala"
        },
        {
          "id": "84684c56-84f3-4c82-bec2-25f84e9d77dd",
          "label": "1 Tbsp Mayonnaise"
        }
      ],
      "method": [
        {
          "step": 1,
          "text": "Katsu mayo time. Add mayo, veggie stock cube, garam masala, curry powder and runny honey to a bowl. Mix well and set aside."
        },
        {
          "step": 2,
          "text": "Zingy slaw. Grate your radishes and half a cabbage. Add to a bowl with sesame oil, the juice of a lime and a large handful of chopped coriander. Mix everything together and set aside."
        },
        {
          "step": 3,
          "text": "Chicken time. Get three bowls out. Add flour to one, eggs to another (whisk them up) and breadcrumbs to the third. Dip your chicken thigh in flour, followed by the eggs and then the breadcrumbs."
        },
        {
          "step": 4,
          "text": "Get a pan on the heat and add a very large glug of vegetable oil. Once the oil is hot, add your chicken thighs. Cook for 4 minutes on each side until the breadcrumbs are golden. Keep turning the chicken until it is cooked through."
        },
        { "step": 5, "text": "Toast your brioche buns." },
        {
          "step": 6,
          "text": "Assemble time. Add a very large dollop of katsu mayo to your bun. Add your chicken. Add a large spoonful of the zingy slaw. Dollop some more katsu mayo on the top half of the bun and place that on top of the burger. Take a bite and enjoy!"
        }
      ]
    },
